{
  "name": "@gulppress/setup",
  "private": false,
  "version": "0.3.3",
  "description": "",
  "keywords": [
    "browser-sync",
    "bundler",
    "webpack",
    "wordpress",
    "wordpress-bundler"
  ],
  "homepage": "https://github.com/wwwoda/gulppress",
  "license": "MIT",
  "author": "Woda Digital",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "lib/index.js",
  "files": [
    "lib",
    "templates"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:js": "babel ./src --out-dir lib --extensions '.ts,.tsx'",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf './lib' && rm -f ./tsconfig.tsbuildinfo",
    "lint": "eslint src --ext='.ts,.js'",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "change-case": "^3.1.0",
    "download": "^7.1.0",
    "execa": "^3.3.0",
    "find-up": "^4.1.0",
    "handlebars": "^4.5.3",
    "inquirer": "^7.0.0",
    "node-env-file": "^0.1.8",
    "ora": "^4.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "respository": "https://github.com/wwwoda/gulppress",
  "gitHead": "fcf186c6950335318a7d476fce48b1cbc3e3c841"
}
