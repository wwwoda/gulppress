{
  "name": "@gulppress/cli",
  "private": false,
  "version": "0.1.1-alpha.0",
  "description": "",
  "keywords": [
    "browser-sync",
    "bundler",
    "webpack",
    "wordpress",
    "wordpress-bundler"
  ],
  "homepage": "https://github.com/wwwoda/gulppress",
  "license": "MIT",
  "author": "Woda Digital",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "gulppress-cli": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:js": "babel ./src --out-dir lib --extensions '.ts,.tsx'",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf './lib' && rm -f ./tsconfig.tsbuildinfo",
    "lint": "eslint src --ext='.ts,.js'",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "download": "^7.1.0",
    "inquirer": "^7.0.0",
    "inquirer-fuzzy-path": "^2.1.0",
    "ora": "^4.0.2",
    "replace-in-file": "^4.2.0"
  },
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "release-it": "^12.3.6",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "respository": "https://github.com/wwwoda/gulppress",
  "gitHead": "b2064843da8b1f3f658e6ef19697d6b05e33dd81"
}
